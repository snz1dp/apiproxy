{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ApiProxy",
      "type": "debugpy",
      "request": "launch",
      "module": "openaiproxy.main",
      "cwd": "${workspaceFolder}/src/apiproxy",
      "args": [
      ],
      "jinja": true,
      "justMyCode": false,
    },
    {
      "name": "Python: Debug Tests",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "purpose": ["debug-test"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/src/backend",
      "justMyCode": false
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}/src/backend",
      "console": "integratedTerminal",
      "env": {
      },
      "envFile": "${workspaceFolder}/src/backend/.env"
    }
  ]
}
